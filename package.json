{
  "private": true,
  "version": "0.0.1",
  "name": "improved-moodle",
  "repository": "https://github.com/guesant/improved-moodle.git",
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "description": "Extensão para navegador criada para melhorar a experiência do usuário na plataforma Moodle.",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "clear": "rm -rf dist",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --fix --ext js,ts,tsx,ts src/",
    "dev": "concurrently -n assets,esbuild \"yarn:assets:dev\" \"yarn:scripts:dev\"",
    "scripts": "./scripts/build.js",
    "scripts:dev": "./scripts/build.js --watch",
    "assets": "gulp assets",
    "assets:dev": "gulp watchAssets",
    "ext:run": "web-ext run",
    "ext:pack": "web-ext build",
    "build": "NODE_ENV=production yarn clear; yarn assets; yarn scripts; yarn ext:pack",
    "prepare": "husky install"
  },
  "webExt": {
    "sourceDir": "dist/ext",
    "artifactsDir": "dist"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@loadable/component": "^5.15.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "css-select": "^4.1.3",
    "deep-equal": "^2.0.5",
    "domutils": "^2.8.0",
    "fuse.js": "^6.4.6",
    "htmlparser2": "^7.1.1",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "normalize-url": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-query": "^3.25.0",
    "react-router-dom": "^5.3.0",
    "react-window": "^1.8.6",
    "ruty": "^1.2.3",
    "use-context-selector": "^1.3.7",
    "use-debounce": "^7.0.0",
    "webextension-polyfill": "^0.8.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-window": "^1.8.5",
    "@types/webextension-polyfill": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "esbuild": "^0.12.25",
    "esbuild-plugin-postcss2": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "gulp": "^4.0.2",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "web-ext": "^6.3.0",
    "yargs": "^17.1.1"
  }
}
