{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "strictNullChecks": true,
    "rootDir": "./lib",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve"
  }
}
